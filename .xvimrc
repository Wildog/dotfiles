set incsearch
set vimregex
set timeoutlen=2000

nmap \bp :xccmd toggleBreakpointAtCurrentLine<CR>
nmap \cm :xcmenucmd Comment Selection<CR>
vmap \cm :xcmenucmd Comment Selection<CR>
nmap \cp :xcmenucmd Copy Method and Function Declarations<CR>
vmap \cp :xcmenucmd Copy Method and Function Declarations<CR>
nmap \<space> :xcmenucmd Remove All Highlighting<CR>
vmap \<space> :xcmenucmd Remove All Highlighting<CR>
nmap \h :xcmenucmd Highlight Occurences of Symbol<CR>
vmap \h :xcmenucmd Highlight Occurences of Symbol<CR>
nmap \cm :xcmenucmd Comment Selection<CR>
vmap \cm :xcmenucmd Comment Selection<CR>

imap <C-e> <Esc>$a

nnoremap <C-o> <C-t>
nnoremap \g <C-]>
nnoremap \d :xccmd jumpToDefinition<CR>
nnoremap \p :xccmd jumpToInstructionPointer<CR>

nmap ]l :nissue<cr>
nmap [l :pissue<cr>
nmap [h :xccmd jumpToPreviousCounterpart<CR>
nmap ]h :xccmd jumpToNextCounterpart<CR>
nnoremap [[ :xcmenucmd Go Back<CR>
nnoremap ]] :xcmenucmd Go Next<CR>
